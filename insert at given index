#include<stdio.h>
int main(){
    printf("Name: Preetham G\n USN:1SV24CS100\n ");
    int arr[5]={1,2,3,4,5};
    int n,i,val,pos=2;
    n=5;val=99;
    for(i=n;i>pos;i--)
    {
        arr[i]=arr[i-1];
    }
    arr[pos]=val;
    n++;
    for(i=0;i<n;i++)
    printf("arr[%d]=%d\n",i,arr[i]);
    return 0;
}

output
Name: Preetham G
 USN:1SV24CS100
 arr[0]=1
arr[1]=2
arr[2]=99
arr[3]=3
arr[4]=4
arr[5]=5
